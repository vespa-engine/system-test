# A basic search definition
search music {
  fieldset default {
    fields: title
  }
  document music {
    field tracks type array<string> {
      indexing: summary
    }
    field lang type string{
      indexing: summary
    }
    field title type string {
	indexing: summary | index
    }
  }
}
