schema test {
  document test {
    field props_exact type map<string, string> {
      indexing: index | summary
      struct-field key {
        match: exact
      }
      struct-field value {
        match: exact
      }
    }
    field props_word type map<string, string> {
      indexing: index | summary
      struct-field key {
        match: word
      }
      struct-field value {
        match: word
      }
    }
  }
}
