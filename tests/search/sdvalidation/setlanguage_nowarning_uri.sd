schema setlanguage_nowarning_uri {

  document setlanguage_nowarning_uri {
    field language type string {
      indexing: set_language|summary
    }

    field uri type uri {
      indexing: summary | index
    }

    field title type string {
      indexing: summary | index
    }
  }
  fieldset default {
    fields: title
  }
}