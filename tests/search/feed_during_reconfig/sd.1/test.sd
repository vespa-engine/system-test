schema test {
  document test {
    field f1 type string {
      indexing: summary | attribute | index
    }
    field tags type tag {
      indexing: summary | attribute | index
    }
    field wset type weightedset<string> {
      indexing: summary | attribute | index
    }
    field arraystring type array<string> {
      indexing: summary | attribute | index
    }
    field f2 type string {
      indexing: attribute | summary
    }
  }
}
