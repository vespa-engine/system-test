schema airplanes {
  document airplanes {

    field manufacturer type string {
      indexing: summary | index
    }
    field model type string {
      indexing: summary | index
    }
    field information type string {
      indexing: summary | index
    }
    field popularity type int {
      indexing: attribute
    }
    field rating type double {
      indexing: attribute
    }
    field airlines type array<string> {
      indexing: attribute
    }
  }
}
