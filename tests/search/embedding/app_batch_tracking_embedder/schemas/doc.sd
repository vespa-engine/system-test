schema doc {

  document doc {

    field text type string {
      indexing: index | summary
    }

  }

  field embedding type tensor<float>(x[4]) {
      indexing: input text | embed batch-tracker | attribute | summary
  }

  rank-profile default {
    inputs {
      query(embedding) tensor<float>(x[4])
    }

    first-phase {
      expression: nativeRank
    }

    summary-features {
      query(embedding)
      attribute(embedding)
    }

  }

}
