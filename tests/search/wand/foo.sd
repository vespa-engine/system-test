schema foo {
  document foo {
    field title type string {
      indexing: summary | index
    }
    field desc type string {
      indexing: summary | index
    }
    field tstamp type int {
      indexing: summary | attribute
    }
  }
  fieldset default {
    fields: title, desc
  }
}
