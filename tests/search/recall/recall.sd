schema recall {
  document recall {
    field title type string {
      indexing: summary | index
    }
    field myid type int {
      indexing: summary | attribute
    }
  }
  fieldset default {
    fields: title
  }
  rank-profile default {
    summary-features {
      fieldMatch(title).matches
      queryTermCount
    }
  }
}
