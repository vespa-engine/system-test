schema selsum {
  document selsum {
    field body type string {
      indexing: summary | index
      rank-type: identity
    }
    field foo type string {
      indexing: summary
      summary-to: foosum
    }
    field bar type string {
      indexing: summary
      summary-to: barsum
    }
  }
  fieldset default {
    fields: body
  }
}
