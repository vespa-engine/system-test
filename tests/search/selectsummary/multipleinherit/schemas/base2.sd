schema base2 {

  document base2 {

    field field4 type string {
      indexing: summary | index
    }

    field field5 type string {
      indexing: summary | index
      summary-to: foosum
    }

    field field6 type string {
      indexing: summary | index
      summary-to: barsum
    }

  }
  fieldset default {
    fields: field4, field5, field6
  }

  rank-profile default {
    first-phase { expression: 2 }
  }

}
