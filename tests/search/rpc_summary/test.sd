schema test {
  document test {
    field id type long {
      indexing: summary | attribute
    }
    field f1 type string {
      indexing: summary
    }
  }
  rank-profile rank1 {
    first-phase {
      expression: attribute(id)
    }
    summary-features: attribute(id)
  }
  document-summary s1 {
    summary id type long { source: id }
    summary f1 type string { source: f1 }
  }
}
