schema doublerank {
  
  document doublerank {

    field id type string {
      indexing: summary
    }

    field cluster type int {
      indexing: summary | index
    }

    field rankscore type double {
        indexing: attribute
    }

  }

  rank-profile default {
    first-phase {
        expression: attribute(rankscore)
    }
  }
}

