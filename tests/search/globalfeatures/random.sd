schema random {
  document random {
    field a type string {
      indexing: index | summary
    }
  }

  rank-profile default {
    summary-features { 
      random
      random(1)
      random(2)
      random.match
    }
  }
  fieldset default {
    fields: a
  }

  rank-profile seed inherits default {
    rank-properties {
      random.seed: 100
      random(1).seed: 200
      random(2).seed: 300
    }
  }
}
