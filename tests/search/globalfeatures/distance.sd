schema distance {
  document distance {
    field xy type position {
      indexing: attribute 
    }
  }

  rank-profile default {
    first-phase {
        expression : distance(xy)
    }
    summary-features {
      distance(xy)
      closeness(xy)
    }
    rank-properties {
      closeness(xy).maxDistance: 100
    }
  }
}
