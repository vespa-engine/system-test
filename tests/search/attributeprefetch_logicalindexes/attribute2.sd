schema attribute2 {
  document attribute2 {
    field fieldstring type string {
      indexing: attribute | summary
    }
    field fieldfloat type float {
      indexing: attribute
    }
    field nonattribute type string {
      indexing: summary
    }
    field body type string {
      indexing: summary | index
    }
  }
  fieldset default {
    fields: body
  }

}
