search expsumname {
  document expsumname {
    field flong type long {
      indexing: index | summary
      summary slong {
        source: flong
      }
    }
    field fdouble type double {
      indexing: index | summary
      summary sdouble {
        source: fdouble
      }
    }
    field fstring type string {
      indexing: index | summary
      summary sstring {
        source: fstring
      }
    }
  }
}
