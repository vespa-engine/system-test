search dynsum {
  fieldset default {
    fields: f1, f2
  }
  document dynsum {
    struct sct {
      field s1 type string {}
      field s2 type string {}
    }
    field f1 type array<string> {
      indexing: index | summary f1
      summary-to: default
    }
    field f2 type array<sct> {
      indexing: index | summary f2
    }
  }
  document-summary default {
    summary sum1 {
      source: f1, f2.s1
      dynamic
    }
    summary sum2 {
      source: f1, f2.s1
    }
  }
}
