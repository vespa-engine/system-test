schema test {
    document test {
        field always type string {
            indexing: attribute | summary
        }
        field si type int {
	    indexing: summary
        }
        field sia type int {
	    indexing: attribute | summary
        }
        field sd type double {
	    indexing: summary
        }
        field sda type double {
	    indexing: attribute | summary
        }
        field sdaf type double {
	    indexing: attribute | summary
            attribute: fast-search
        }
        field sdaff type double {
	    indexing: attribute | summary
            attribute: fast-search
            rank: filter
        }
    }
}
