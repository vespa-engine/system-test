schema test {
  document test {
    field title type string {
      indexing: summary | index
    }
    field body type string {
      indexing: summary | index
    }
  }
  rank-profile default {
    first-phase {
      expression: fieldMatch(title)
    }
    second-phase {
      expression: fieldMatch(body)
      rerank-count: 200
    }
  }
}
