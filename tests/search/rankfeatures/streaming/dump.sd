schema dump {
  document dump {
    field a type string {
      indexing: index | summary
    }
  }

  rank-profile extra inherits default{
    rank-features {
      term(5).connectedness
      term(5).significance
      term(5).weight
    }
  }

  rank-profile ignore inherits extra {
    ignore-default-rank-features
  }
}
