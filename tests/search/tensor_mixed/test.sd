schema test {
  document test {
    field id type int {
      indexing: attribute | summary
    }
    field mixed type tensor(x{},y[3]) {
      indexing: attribute | summary
    }
  }
  constant model {
    file: search/model.json
    type: tensor(x{},y[3])
  }
  rank-profile default {
    inputs {
        query(tensor) tensor(x{},y[3])
    }
    first-phase {
      expression: sum(query(tensor) * constant(model) * attribute(mixed))
    }
  }
}
