schema indexarray {
  document indexarray {
    field fa type array<string> {
      indexing: index | summary
    }
    field fb type array<string> {
      indexing: attribute | summary
    }
  }
  rank-profile default {
    summary-features {
      fieldInfo(fa).cnt
    }
  }
}
