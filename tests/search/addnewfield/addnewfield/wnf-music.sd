search music {
  
  document music {

    field title type string {
      indexing: summary | index
    }

    field popularity type int {
      indexing: summary | attribute
    }

    field newfield type string {
      indexing: summary | index
    }
  }
  fieldset default {
    fields: title, newfield
  }


  rank-profile default {
    first-phase {
      expression: attribute(popularity)
    }
  }

}
