schema foobar {
  
  fieldset default {
    fields: heading
  }
  document foobar {

    field heading type string {
      indexing: index | summary
    }
    field placement type long {
      indexing: summary | attribute
    }
  }

  rank-profile default {
    first-phase {
      expression: 1000 * nativeRank - attribute(placement)
    }
  }

}

