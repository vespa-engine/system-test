schema doc {

  field indexed_at_seconds type long {
    indexing: now | summary | attribute
  }

  document doc {

    field label type string {
      indexing: index | summary
    }

    field count type int {
      indexing: attribute | summary
    }

    field text type string {
      indexing: index | summary
    }

  }

}
