<?xml version="1.0" encoding="utf-8" ?>
<services version="1.0">
  <admin version="2.0">
    <adminserver hostalias="node1"/>
  </admin>
  <container id="search" version="1.0">
    <config name="language.lib.langdetect.langdetect"> <!-- not defined for all platforms -->
      <enabled>true</enabled>
    </config>
    <nodes jvmargs=" -verbose:gc -Xms8g -Xmx8g -XX:NewRatio=1 -XX:+PrintGCDetails ">
    <!--<nodes jvmargs="-agentlib:yjpagent -verbose:gc -Xms8g -Xmx8g -XX:NewRatio=1 -XX:+PrintGCDetails">-->
      <node hostalias='node1'/>
    </nodes>
    <search>
      <provider id="mock">
        <searcher id="com.yahoo.performance.searcher.BackEndSearcher" bundle="performance"/>
      </provider>
    </search>
    <document-api/>
    <accesslog type="disabled"/>
  </container>
  <service name="devnull" command="$VESPA_HOME/bin/vespa-destination --instant --silent 1000000000" version="1.0">
    <node hostalias="node1"/>
  </service>  
</services>
